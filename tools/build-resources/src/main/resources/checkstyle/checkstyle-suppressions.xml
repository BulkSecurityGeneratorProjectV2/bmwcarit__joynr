<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.1//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>
    <suppress checks="." files="[\\/]R\.java$"/>
	<suppress checks="ParameterNumber" files="[\\/]generated[\\/]"/> <!-- We can't influence how many parameters the IDL-Creator defines -->
	<suppress checks="LineLength" files="[\\/]generated[\\/]"/>      <!-- Formatting generated code is not worth the work-->
	<suppress checks=".*Name.*" files="[\\/]generated[\\/].*"/>        <!-- Can't controll names of generated methods and params -->
	<suppress checks="RedundantImport" files="[\\/]generated[\\/].*"/>  		
	<suppress checks="UnusedImports" files="[\\/]generated[\\/].*"/>     		
	
	<suppress checks="." files="[\\/]generated[\\/].*"/> <!--  For a start - deactivating all checkstyle tests in generated code -->
		
	<suppress checks="." files="[\\/]fibex[\\/]"/>
	<suppress checks="." files="\.xml$"/>
	<suppress checks="." files="\.js$"/>
	<suppress checks="." files="[\\/]demoApplications[\\/].*"/>
	<suppress checks="." files="[\\/]demo_applications[\\/].*"/>
			
			
</suppressions>