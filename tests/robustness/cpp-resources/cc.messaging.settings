channel-url-directory-url=http://@JOYNR_SERVER_HOST@:@JOYNR_SERVER_HTTP_PORT@/discovery/channels/discoverydirectory_channelid/
channel-url-directory-channelid=discoverydirectory_channelid
channel-url-directory-participantid=channelurldirectory_participantid

capabilities-directory-url=http://@JOYNR_SERVER_HOST@:@JOYNR_SERVER_HTTP_PORT@/discovery/channels/discoverydirectory_channelid/
