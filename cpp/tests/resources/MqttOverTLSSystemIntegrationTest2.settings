[messaging]
broker-url=mqtt://@JOYNR_SERVER_HOST@:@JOYNR_SERVER_MQTT_TLS_PORT@

discovery-directories-domain=io.joynr

capabilities-directory-url=" "
capabilities-directory-channelid={"_typeName":"joynr.system.RoutingTypes.MqttAddress","brokerUri":"@JOYNR_DEFAULT_GBID@","topic":"discoverydirectory_channelid/low/capabilitiesdirectory_participantid"}
capabilities-directory-participantid=capabilitiesdirectory_participantid

create-channel-retry-interval=500
send-msg-retry-interval=500
local-proxy-host=
local-proxy-port=
index=0
longpoll-retry-interval=5000
delete-channel-retry-interval=5000
persistence-file=joynr-systemintegrationtest2.settings

[capabilitiesClient]
fakeDomain=integrationDomain
fakeInterfaceName=integrationInterfaceName
fakeChannelId=somerandomChannelId1
fakeParticipantId=Fake_ParticipantId

[cluster-controller]
mqtt-certificate-authority-pem-filename=/data/ssl-data/certs/ca.cert.pem
mqtt-certificate-pem-filename=/data/ssl-data/certs/client.cert.pem
mqtt-private-key-pem-filename=/data/ssl-data/private/client.key.pem
mqtt-tls-enabled=true
