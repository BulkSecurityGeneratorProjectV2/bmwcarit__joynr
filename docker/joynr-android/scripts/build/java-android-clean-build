#/bin/bash

cd /data/src

# fail on first error
# exit immediately if a command exits with a non-zero status
# print commands before they are executed
set -e

./joynr-setup-android-sdk.sh

mvn clean package -P no-license-and-notice,no-java-formatter,no-checkstyle,android \
-DskipTests=false \
-Dmaven.test.failure.ignore=true \
-Denforcer.skip=true -Dmaven.compile.fork=true \
-Dmaven.junit.fork=true