#!/bin/bash

set -e
(
  echo "installing Franca dependencies not found in Maven Central"
  cd /data/dependencies/joynr/tools/generator/dependency-libs
  mvn install
)

(
  cd /data/src/javascript
  mvn clean install \
  -Djslint.failOnError=true \
  -Dskip.copy-notice-file=true \
  -Dskip.unpack-license-info=true \
  -DskipTests=false
)