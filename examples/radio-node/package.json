{
  "name": "radio-node",
  "version": "0.16.0-SNAPSHOT",
  "description": "simple app to demonstrate usage of joynr",
  "main": "src/main/js/radioConsumer.js",
  "private": true,
  "dependencies": {
  },
  "config": {
    "domain": "nodeProvider",
    "cc": {
      "host": "localhost",
      "port": "4242"
    }
  },
  "scripts": {
    "preinstall": "npm install ../../javascript/libjoynr-js/target/classes",
    "startconsumer" : "node src/main/js/radioConsumer.js $npm_package_config_domain $npm_package_config_cc_host $npm_package_config_cc_port",
    "startprovider" : "node src/main/js/radioProvider.js $npm_package_config_domain $npm_package_config_cc_host $npm_package_config_cc_port"
  }
}
